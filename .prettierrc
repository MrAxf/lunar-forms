{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "tailwindFunctions": [
    "tw"
  ],
  "importOrder": ["<THIRD_PARTY_MODULES>", "^@/(.*)$","^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
